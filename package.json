{
  "name": "paperback",
  "version": "0.0.0-semantic-release",
  "description": "Easily generate files from templates",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "scripts": {
    "compile": "babel src --out-dir lib",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./ --ignore-pattern node_modules/ --ignore-pattern lib",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "npm run lint && npm run compile && nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dustinspecker/paperback.git"
  },
  "keywords": [
    "paperback",
    "generate",
    "scaffold",
    "template"
  ],
  "author": {
    "name": "Dustin Specker",
    "email": "DustinSpecker@DustinSpecker.com",
    "url": "github.com/dustinspecker"
  },
  "license": "MIT",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "a": "^1.0.2",
    "austin": "^0.1.1",
    "ava": "^0.15.1",
    "babel-cli": "^6.4.0",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "coveralls": "^2.11.6",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^2.6.0",
    "eslint-config-dustinspecker": "^1.0.0",
    "eslint-plugin-ava": "^2.2.1",
    "eslint-plugin-no-use-extend-native": "^0.3.1",
    "eslint-plugin-xo": "^0.4.0",
    "nyc": "^6.1.1",
    "proxyquire": "^1.7.4",
    "semantic-release": "^4.3.5"
  },
  "ava": {
    "files": [
      "tests/test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "exclude": [
      "tests"
    ]
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "inquirer": "^3.0.2",
    "lodash": "^4.6.1",
    "meow": "^3.7.0",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.0.1",
    "pify": "^2.3.0",
    "recursive-readdir": "^2.0.0",
    "string-replace-with-object": "^1.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
